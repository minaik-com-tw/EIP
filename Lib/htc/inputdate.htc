	else if(datestr.indexOf(",") >=0 )
		dateSplit = ",";
	else if(datestr.indexOf("-") >=0 )
		dateSplit = "-";
		
	//else //20040506
	
	if (dateSplit != "")
	{
		var date_array = datestr.split(dateSplit)
		if (date_array.length!=3)
		{
			alert("Date value is wrong !");
			return false;
		}
		else
		{
			var Datestr1 = ""
			for (var j=0 ;j<=2;j++)
			{
				if (j==iyear)
					Datestr1+= date_array[iyear] +"/";
				else if(j==imonth)
					Datestr1+= date_array[imonth] +"/";
				else if(j==iday)
					Datestr1+= date_array[iday] +"/";
			}
			if (Datestr1.charAt(Datestr1.length-1) == '/' ) Datestr1=Datestr1.substring(0,Datestr1.length-1);
		}
	}
	else
	{
		var Datestr1 = ""
		if((datestr.length!=8))
			alert("Date value is wrong !");
		else
		{	
			if(format == "yyyy/mm/dd")
			{
				Datestr1 = datestr.substring(0,4)+"/"+datestr.substring(4,6)+"/"+datestr.substring(6,8);
			}
			else if (format == "mm/dd/yyyy")
			{
				Datestr1 = datestr.substring(0,2)+"/"+datestr.substring(2,4)+"/"+ datestr.substring(4,8)
			}
			else if (format == "dd/mm/yyyy")
			{
				Datestr1 =  datestr.substring(0,2)+"/"+datestr.substring(2,4)+"/"+ datestr.substring(4,8)
			}
		}
	//	alert(Datestr1);
	}
	
	object.value = Datestr1;
	value=Datestr1;
	
	
	//----------- 檢驗日期是否正確
	if(regexp.test(value))
	{
		dateArray=value.match(regexp);

		year=dateArray[iyear];
		month=dateArray[imonth];
		day=dateArray[iday];

		//alert("The Date you have filled is:\nYear:"+year+"\nMonth:"+month+"\nDay:"+day);
		  
	/*	if(year<2001)
		{
			alert("Year must be greater than 2001!");
			return false;
		}*/
		if(month<0||month>12)
		{
			alert("Month must range from 1 to 12!");
			return false;
		}

		if(day<0||day>31)
		{
			alert("Day must range from 1 to 31!");
			return false;
		}
		else
		{ 
			if(month==2)
			{ 
				if(isLeapYear(year)&&day>29)
				{
					alert("In Month 2,Day must range from 1 to 29!");
					return false;
				}
				if(!isLeapYear(year)&&day>28)
				{
					alert("In Month 2,Day must range from 1 to 28!");
					return false;
				}
			}

			if((month==4||month==6||month==9||month==11)&&(day>30))
			{
				alert("In this Month ,Day must range from 1 to 30!");
				return false;
			}
		}

	}
	else
	{
		alert("Date isn't matched with Format!\nDate Format:"+format);
		return false;
	}

	return true;
}

