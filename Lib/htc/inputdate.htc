function Next()
{
	if ( parseInt(this.Month) >= 11 ) 
		BuildCalendar(this.Year+1, 0 );
	else
		BuildCalendar(this.Year, this.Month +1 );
}

function Prev()
{
	if ( parseInt(this.Month) <= 0  ) 
		BuildCalendar(this.Year-1, 11 );
	else
		BuildCalendar(this.Year, this.Month - 1 );
}

